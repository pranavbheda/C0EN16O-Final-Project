package finalProject;

import java.awt.*;
import java.awt.event.*;
import javax.swing.*;


@SuppressWarnings("serial")
public class DisplayNew extends JFrame implements ActionListener {

	RMOS rmos = new RMOS();
	Graph g = new Graph();
	RCM rcm1 = new RCM("San Jose", "001");
	RCM rcm2 = new RCM("San Fran", "002");
	
	public DisplayNew(String titleText) {
		super(titleText);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setLayout(new GridLayout(2,1));
		
		JPanel rmosPanel = new JPanel();
		JButton h = new JButton("Hello");
		JButton w = new JButton("World");
		
		super.add(h); super.add(w);
		

		
		rmos.addRCM(rcm1);	
		rmos.addRCM(rcm2);

		
		setSize(600, 600);
		setVisible(true);
        setLocationRelativeTo(null);

	}

	@Override
	public void actionPerformed(ActionEvent e) {
		// TODO Auto-generated method stub
		
	}

}
